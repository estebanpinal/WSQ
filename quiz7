#include <iostream>
using namespace std;
long fibonaci(long x){
  long a=0; long b=1; long c=0;
    while (x>1){
    c=a+b; a=b; b=c;
    x=x-1;}
    return c;
  }
int main (){
  long x;
  cout << "Numero: " << endl;
  cin >> x;
    if (x==1){
      int f = 1;
      cout << f;
  }
  else {
    cout << fibonaci(x) << endl;}
    return 0;}
